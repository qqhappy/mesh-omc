<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xinwei.minas.enb.core.model.stat.EnbRealtimeItemConfig"
		table="enb_realtime_config">
		<id name="itemId" type="int">
			<column name="itemId" not-null="true" />
			<generator class="assigned" />
		</id>
		<property name="tagId" type="int">
			<column name="tagId" not-null="true" />
		</property>
		<property name="statObject" type="java.lang.String">
			<column name="statObject" not-null="true" />
		</property>
		<property name="reportSystem" type="java.lang.String">
			<column name="reportSystem" not-null="false" />
		</property>
		<property name="measureType" type="java.lang.String">
			<column name="measureType" not-null="false" />
		</property>
		<property name="measureEvent" type="java.lang.String">
			<column name="measureEvent" not-null="false" />
		</property>
		<property name="name_zh" type="java.lang.String">
			<column name="name_zh" not-null="true" />
		</property>
		<property name="name_en" type="java.lang.String">
			<column name="name_en" not-null="true" />
		</property>
		<property name="desc_zh" type="java.lang.String">
			<column name="desc_zh" not-null="false" />
		</property>
		<property name="desc_en" type="java.lang.String">
			<column name="desc_en" not-null="false" />
		</property>
		<property name="exp" type="java.lang.String">
			<column name="exp" not-null="false" />
		</property>
		<property name="unit" type="java.lang.String">
			<column name="unit" not-null="false" />
		</property>
		<property name="dataType" type="java.lang.String">
			<column name="dataType" not-null="true" />
		</property>
		<property name="timeType" type="java.lang.String">
			<column name="timeType" not-null="true" />
		</property>
		<property name="spaceType" type="java.lang.String">
			<column name="spaceType" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
