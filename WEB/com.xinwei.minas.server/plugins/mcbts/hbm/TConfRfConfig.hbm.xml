<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xinwei.minas.mcbts.core.model.layer1.RFConfig"
		table="mcbts_rfconfig">
		<id name="idx" type="long">
			<column name="idx" not-null="true" />
			<generator class="assigned" />
		</id>
		<property name="moId" type="long">
			<column name="moId" not-null="true"/>
		</property>
		<property name="freqOffset" type="int">
			<column name="freqOffset" not-null="true"/>
		</property>
		<property name="antennaPower" type="int">
			<column name="antennaPower" not-null="true"/>
		</property>
		<property name="rxSensitivity" type="int">
			<column name="rxSensitivity" not-null="true"/>
		</property>
		<property name="cableLoss" type="int">
			<column name="cableLoss" not-null="true"/>
		</property>
		<property name="psLoss" type="int">
			<column name="psLoss" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>
