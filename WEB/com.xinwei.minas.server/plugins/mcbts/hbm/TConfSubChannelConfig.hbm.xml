<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xinwei.minas.mcbts.core.model.layer2.SCGChannelConfigItem"
		table="mcbts_airlink_subscgchannel">
		<id name="idx" type="long">
			<column name="idx" not-null="true" />
			<generator class="assigned" />
		</id>
		<property name="moId" type="long">
			<column name="moId" not-null="true"/>
		</property>
		<property name="channelType" type="int">
			<column name="channelType" not-null="true"/>
		</property>
		<property name="tsIndex" type="int">
			<column name="tsIndex" not-null="true"/>
		</property>
		<property name="scgIndex" type="int">
			<column name="scgIndex" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>
