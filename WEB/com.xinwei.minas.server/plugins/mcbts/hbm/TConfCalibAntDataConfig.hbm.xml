<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xinwei.minas.mcbts.core.model.layer1.AntennaCalibrationConfig"
		table="mcbts_calibantdataconfig">
		<id name="idx" type="long">
			<column name="idx" not-null="true" />
			<generator class="assigned" />
		</id>
		<property name="moId" type="long">
			<column name="moId" not-null="true"/>
		</property>
		<property name="antennaIndex" type="int">
			<column name="antennaIndex" not-null="true"/>
		</property>
		<property name="dataType" type="int">
			<column name="dataType" not-null="true"/>
		</property>
		<!-- 
		<property name="detailInfo" type="java.sql.Blob">
			<column name="detailInfo" not-null="true"/>
		</property>
		-->
	</class>
</hibernate-mapping>
