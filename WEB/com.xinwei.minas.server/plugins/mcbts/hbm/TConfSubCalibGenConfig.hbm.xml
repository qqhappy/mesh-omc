<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xinwei.minas.mcbts.core.model.layer1.CalibGenConfigItem"
		table="mcbts_calibgenconfig_subitem">
		<id name="idx" type="long">
			<column name="idx" not-null="true" />
			<generator class="assigned" />
		</id>
		<property name="moId" type="long">
			<column name="moId" not-null="true"/>
		</property>
		<property name="antennaIndex" type="int">
			<column name="antennaIndex" not-null="true"/>
		</property>
		<property name="rxGain" type="int">
			<column name="rxGain" not-null="true"/>
		</property>
		<property name="txGain" type="int">
			<column name="txGain" not-null="true"/>
		</property>
		<property name="calibrationResult" type="int">
			<column name="calibrationResult" not-null="true"/>
		</property>
		<property name="predH" type="java.lang.String">
			<column name="predH" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>
